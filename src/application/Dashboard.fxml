<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="200.0" prefWidth="213.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DashboardController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="273.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="welcomeLabel" layoutY="6.0" text="Welcome to your dashboard!">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
   <left>
      <AnchorPane prefHeight="200.0" prefWidth="180.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="28.0" layoutY="14.0" text="Account Actions">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="historyButton" layoutX="55.0" layoutY="47.0" mnemonicParsing="false" text="View History" />
            <Button fx:id="logoutButton" layoutX="71.0" layoutY="150.0" mnemonicParsing="false" onAction="#logOut" text="Log out" />
            <Button fx:id="depositButton" layoutX="47.0" layoutY="80.0" mnemonicParsing="false" text="Make a Deposit" />
            <Button fx:id="transferButton" layoutX="49.0" layoutY="113.0" mnemonicParsing="false" text="Transfer funds" />
         </children>
      </AnchorPane>
   </left>
</BorderPane>
